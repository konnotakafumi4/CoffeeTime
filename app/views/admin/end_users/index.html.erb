<% @end_users.each do |end_user| %>
  <div class="end_user-list">
    <%= link_to admin_end_user_path(end_user) do %>
      <%= image_tag end_user.get_profile_image(100,100), class: "rounded-circle" %>
      <%= end_user.name %>
    <% end %>
    <%= end_user.introduction %>
    <% if end_user.is_deleted == true %>
      <span class="text-secondary" ><strong>退会</strong></span>
    <% else %>
      <span class="text-success" ><strong>有効</strong></span>
    <% end %>
  </div>
<% end %>