<h1>編集画面</h1>

<%= form_with model: @end_user, url: public_end_user_path, method: :patch do |f| %>
<h4>画像</h4>
  <%= f.file_field :end_user_image, class: "form-control-file user_image", accept: 'image/*' %>

<h4>名前</h4>
  <%= f.text_field :name %>

<h4>紹介文</h4>
  <%= f.text_area :introduction%>

  <%= f.submit '保存' %>

<% end %>