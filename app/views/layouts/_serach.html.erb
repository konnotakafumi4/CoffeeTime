<% if end_user_signed_in? %>
  <div class="search_form w-100 text-right fixed-top">
    <%= form_with url: search_path, local: true, method: :get do |f| %>
      <%= f.text_field :word %>
      <%= f.select :range, options_for_select([['EndUser'], ['Coffee']]) %>
      <%= f.submit "検索", class: "btn btn-primary" %>
    <% end %>
  </div>
<% end %>